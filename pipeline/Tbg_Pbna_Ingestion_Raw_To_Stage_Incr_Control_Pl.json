							"FileName": {
								"value": "@concat(activity('Get_Raw_To_Publish_Details').output.firstRow.RawFilePattern,activity('Get_Raw_To_Publish_Details').output.firstRow.LoadFrequency,'*')",
								"type": "Expression"
							}
						}
					},
					"fieldList": [
						"childItems",
						"itemType"
					],
					"storeSettings": {
						"type": "AzureBlobStorageReadSettings",
						"recursive": true
					},
					"formatSettings": {
						"type": "BinaryReadSettings"
					}
				}
			}
		],
		"parameters": {
