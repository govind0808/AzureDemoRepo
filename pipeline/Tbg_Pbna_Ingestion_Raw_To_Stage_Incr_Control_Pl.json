							"dependsOn": [],
							"policy": {
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "Tbg_Pbna_Ingestion_Raw_To_Stage_Incremental_Pl",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true,
